{
  "name": "artfullylondon-web-admin",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "repository": "https://github.com/stevejay/artfullylondon-web-admin",
  "scripts": {
    "build": "rimraf ./build && NODE_ENV=production webpack -p --bail",
    "serve": "firebase serve --project artfullylondon-prod-admin",
    "deploy": "firebase deploy --project artfullylondon-prod-admin",
    "start": "cross-env-shell PORT=$(get-port) UI_PORT=$(get-port) node ./browser-sync.js",
    "start:defaults": "cross-env-shell node ./browser-sync.js",
    "lint": "standard --fix",
    "test": "jest --no-cache",
    "build-analyse": "webpack-bundle-analyzer build/stats.json",
    "test-ci": "jest --coverage --runInBand && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "cypress:open": "cypress open"
  },
  "devEngines": {
    "node": "8.x || 9.x"
  },
  "jest": {
    "setupFiles": [
      "./jest/jest-raf-shim.js",
      "./jest/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test___"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/modules/user/lib/cognito-user.js",
      "!src/shared/lib/fetch-impl.js",
      "!src/shared/lib/normalise.js",
      "!src/shared/lib/saga.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "./node_modules",
      "./build"
    ],
    "env": {
      "jest": true
    },
    "globals": [
      "shallow",
      "mount",
      "cy"
    ]
  },
  "devDependencies": {
    "@types/googlemaps": "^3.30.7",
    "@types/history": "^4.6.2",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.24.4",
    "cheerio": "^1.0.0-rc.2",
    "connect-history-api-fallback": "^1.5.0",
